#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int a;
    scanf("%d", &a);
    int firstDigit = a/1000;
    int lastDigit = a%10;
    int result = firstDigit + lastDigit;

    printf("%d", result);
    return 0;
}
